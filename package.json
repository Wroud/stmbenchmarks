{
  "name": "stmbenchmarks",
  "version": "1.0.0",
  "description": "Benchmarks for js state managers like redux and etc.",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "src",
    "lib",
    "lib/index.d.ts"
  ],
  "keywords": [
    "react",
    "typescript",
    "state",
    "functional",
    "immutable",
    "redux",
    "reistore",
    "effector",
    "stm"
  ],
  "author": "Alexey Potsetsuev",
  "license": "MIT",
  "scripts": {
    "publish:beta": "npm run build & npm publish -tag beta",
    "publish:alpha": "npm run build & npm publish -tag alpha",
    "publish:rc": "npm run build & npm publish -tag rc",
    "publish:latest": "npm run build & npm publish",
    "build": "tsc",
    "bench": "ts-node ./src/runner.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Wroud/stmbenchmarks.git"
  },
  "release": {
    "branch": "master"
  },
  "publishConfig": {
    "tag": "latest"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^10.3.4",
    "@types/benchmark": "^1.0.31",
    "effector": "^0.18.0-beta.8",
    "redux": "*",
    "ts-node": "^5.0.1",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2",
    "reselect": "3.0.1",
    "benchmark": "^2.1.4",
    "reistore": "1.2.2"
  }
}
